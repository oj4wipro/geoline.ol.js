{
  "name": "@stadtmessungsamt-stuttgart/geoline.ol.js",
  "version": "2.1.0",
  "description": "Bibliothek zur einfachen Einbindung von Kartendiensten des Stadtmessungsamtes Stuttgart in OpenLayers",
  "author": "Stadtmessungsamt, LHS Stuttgart",
  "type": "module",
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md"
  ],
  "main": "dist/geoline.ol.js",
  "types": "dist/geoline.ol.d.ts",
  "style": "dist/geoline.ol.css",
  "dependencies": {
    "core-js": "^3.45.1",
    "jsonp": "^0.2.1",
    "ol": "^10.6.1",
    "proj4": "^2.7.0"
  },
  "devDependencies": {
    "@stadtmessungsamt-stuttgart/geoline.ol.js": "file:./",
    "concurrently": "^9.1.2",
    "copy-webpack-plugin": "^12.0.2",
    "cors-anywhere": "^0.4.4",
    "cross-var": "^1.1.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "cypress": "^15.2.0",
    "gh-pages": "^6.3.0",
    "mini-css-extract-plugin": "^2.9.4",
    "replace": "^1.2.2",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.9.2",
    "wait-on": "^8.0.4",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stadtmessungsamt-stuttgart/geoline.ol.js"
  },
  "license": "CC-BY-NC-SA-4.0",
  "scripts": {
    "audit": "npm audit --only=prod",
    "build-dist": "webpack --config webpack.dist.config.cjs --mode production",
    "build-dev": "webpack --config webpack.dist.config.cjs --watch",
    "build-examples": "npm run build-dist && webpack --config webpack.examples.config.cjs",
    "serve-examples": "wait-on demo && webpack serve --config webpack.examples.config.cjs --port 8080 --open",
    "deploy-gh-pages": "npm run build-examples && gh-pages --dist demo",
    "cypress-open": "concurrently -k -s first \"webpack serve --config webpack.examples.config.cjs --port 8080\" \"wait-on http://localhost:8080 && cypress open\"",
    "cypress-run": "concurrently -k -s first \"webpack serve --config webpack.examples.config.cjs --port 8080\" \"wait-on http://localhost:8080 && cypress run --browser chrome\"",
    "start-dev": "concurrently \"npm run build-dev\" \"webpack --config webpack.examples.config.cjs --watch\" \"npm run serve-examples\""
  }
}
